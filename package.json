{
    "name": "nuclearest-js-storage",
    "version": "1.1.3-beta",
    "description": "Smart and flexible Web Storage with fallback strategy",
    "publishConfig": {
      "registry": "https://npm.registry.com/"
    },
    "scripts": {
      "build": "scripty",
      "doc": "node .internal/build-doc github",
      "doc:site": "node .internal/build-doc site",
      "watch": "scripty",
      "test": "cross-env NODE_ENV=test nyc mocha -R spec --compilers js:babel-register  --recursive && nyc report --reporter=text-lcov | coveralls",
      "test:noCoverall": "cross-env NODE_ENV=test nyc mocha -R spec --compilers js:babel-register  --recursive",
      "test:watch": "npm test -- --watch ",
      "test:watchNoCoverall": "npm run test:noCoverall -- --watch"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/harlott/nuclearest-js.git"
    },
    "keywords": [
      "javascript",
      "ES6",
      "localStorage",
      "sessionStorage",
      "cookie",
      "fallback storage",
      "custom storage",
      "universal"
    ],
    "author": "harlott",
    "license": "MIT",
    "devDependencies": {
      "autoprefixer": "^7.2.3",
      "autoprefixer-loader": "^3.2.0",
      "babel-cli": "^6.11.4",
      "babel-core": "^6.22.1",
      "babel-eslint": "^8.0.3",
      "babel-loader": "^7.1.2",
      "babel-plugin-add-module-exports": "^0.2.1",
      "babel-plugin-istanbul": "^4.1.5",
      "babel-plugin-transform-async-to-generator": "^6.24.1",
      "babel-plugin-transform-runtime": "^6.22.0",
      "babel-polyfill": "^6.22.0",
      "babel-preset-es2015": "^6.3.13",
      "babel-preset-react-app": "^3.1.0",
      "babel-preset-stage-0": "^6.3.13",
      "babel-register": "^6.22.0",
      "babel-runtime": "^6.20.0",
      "babelify": "^8.0.0",
      "chai": "^4.1.2",
      "chai-as-promised": "^7.1.1",
      "coveralls": "^3.0.0",
      "cross-env": "^5.1.1",
      "docdown": "^0.7.2",
      "eslint": "^4.13.1",
      "fs-extra": "^5.0.0",
      "jsdoc": "^3.5.5",
      "mocha": "^4.0.1",
      "nyc": "^11.3.0",
      "scripty": "^1.7.0",
      "transform-runtime": "0.0.0",
      "webpack": "^3.10.0"
    },
    "dependencies": {
      "lodash": "^4.17.4",
      "require": "^2.4.20"
    },
    "nyc": {
      "sourceMap": false,
      "instrument": false
    }
  }
  